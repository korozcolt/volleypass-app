{
  "expo": {
    "name": "volleypass-app",
    "slug": "volleypass-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-volley_pass_white_back.png",
    "scheme": "volleypassapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-volley_pass_white_back.png",
        "backgroundColor": "#1976d2"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/logo-volley_pass_white_back.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo-volley_pass_white_back.png",
          "imageWidth": 250,
          "resizeMode": "contain",
          "backgroundColor": "#1976d2"
        }
      ]
    ],
    "extra": {
      "API_BASE_URL": "http://localhost:8000/api/v1",
      "PUSHER_APP_KEY": "4a23367f593455b21847",
      "PUSHER_CLUSTER": "us2",
      "PUSHER_FORCE_TLS": "true",
      "PUSHER_AUTH_ENDPOINT": "http://localhost:8000/api/broadcasting/auth",
      "ENVIRONMENT": "development"
    }
  }
}
