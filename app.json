{
  "expo": {
    "name": "volleypass-app",
    "slug": "volleypass-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-volley_pass_white_back.png",
    "scheme": "volleypassapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-volley_pass_white_back.png",
        "backgroundColor": "#1976d2"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo-volley_pass_white_back.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo-volley_pass_white_back.png",
          "imageWidth": 250,
          "resizeMode": "contain",
          "backgroundColor": "#1976d2"
        }
      ]
    ]
  }
}
